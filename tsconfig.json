{
  "compilerOptions": {
    "allowJs": false,
    "module": "esnext",
    "esModuleInterop": true,
    "types": [],
    "target": "es6",
    "sourceMap": true,
    "noEmitOnError": false,
    "jsx": "react",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": false,
    "noUnusedLocals": false,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": false,
    "strict": false,
    "noEmit": false,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules",
    ".idea",
    "outputFiles"
  ],
  "lib": [
    "dom",
    "es2015",
    "es2017"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
